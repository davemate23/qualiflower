<p id="notice"><%= notice %></p>

<h1>Accreditations</h1>

<table>
  <thead>
    <tr>
      <th>Ukprn</th>
      <th>Pubukprn</th>
      <th>Kiscourse</th>
      <th>Accurl</th>
      <th>Acctext</th>
      <th>Acctype</th>
      <th>Kismode</th>
      <th>Accdepend</th>
      <th>Accdependurl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @accreditations.each do |accreditation| %>
      <tr>
        <td><%= accreditation.ukprn %></td>
        <td><%= accreditation.pubukprn %></td>
        <td><%= accreditation.kiscourse %></td>
        <td><%= accreditation.accurl %></td>
        <td><%= accreditation.acctext %></td>
        <td><%= accreditation.acctype %></td>
        <td><%= accreditation.kismode %></td>
        <td><%= accreditation.accdepend %></td>
        <td><%= accreditation.accdependurl %></td>
        <td><%= link_to 'Show', accreditation %></td>
        <td><%= link_to 'Edit', edit_accreditation_path(accreditation) %></td>
        <td><%= link_to 'Destroy', accreditation, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Accreditation', new_accreditation_path %>
